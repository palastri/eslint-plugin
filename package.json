{
  "name": "@palastri/eslint-plugin",
  "version": "0.0.0",
  "description": "ESLint plugin from personal taste of Palastri maintainer",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "author": "Imam S Rifkan <imamsrifkan@gmail.com>",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/palastri/eslint-plugin.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "lint": "eslint --cache .",
    "prepack": "rimraf dist && yarn build",
    "prettier:check": "prettier --check package.json tsconfig.json '.github/**' .yarnrc.yml",
    "prettier:write": "prettier --write package.json tsconfig.json '.github/**' .yarnrc.yml",
    "typechecks": "tsc -p ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.19.8",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.0",
    "typescript": "^5.3.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "LICENSE",
    "dist",
    "package.json"
  ],
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.0.2"
}
